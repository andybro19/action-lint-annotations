{
    "problemMatcher": [
        {
            "owner": "pycodestyle",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.+?):(\\d+):(\\d+): ([A-EG-Z]\\d+) (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
