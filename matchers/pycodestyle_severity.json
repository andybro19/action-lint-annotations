{
    "problemMatcher": [
        {
            "owner": "pycodestyle_severity",
            "pattern": [
                {
                    "regexp": "^##\\[([a-z]*)\\](\\S*):(\\d+):(\\d+): ([A-Z]\\d+) (.*)$",
                    "severity": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}
