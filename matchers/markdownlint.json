{
    "problemMatcher": [
        {
            "owner": "markdownlint",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.+?):(\\d+)(?::(\\d+))? (MD\\d+)(?:/\\S+)? (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
