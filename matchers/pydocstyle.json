{
    "problemMatcher": [
        {
            "owner": "pydocstyle",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.+?):(\\d+) in .+$",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "\\s*(D\\d+): (.+)$",
                    "code": 1,
                    "message": 2
                }
            ]
        }
    ]
}
