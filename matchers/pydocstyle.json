{
    "problemMatcher": [
        {
            "owner": "pydocstyle",
            "severity": "warning",
            "column": 1,
            "pattern": [
                {
                    "regexp": "^(\\S*):(\\d*) .*",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "\\s*(D\\d*): (.*)$",
                    "message": 2,
                    "code": 1
                }
            ]
        }
    ]
}
