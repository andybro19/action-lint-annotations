{
    "problemMatcher": [
        {
            "owner": "pycodestyle-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.+?):(\\d+):(\\d+): (W\\d+) (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
