{
    "problemMatcher": [
        {
            "owner": "eslint",
            "severity": "notice",
            "pattern": [
                {
                    "regexp": "^(.+)$",
                    "file": 1
                },
                {
                    "regexp": "^  (\\d+):(\\d+)  (error|warning)?\\w*  (.*)$",
                    "line": 1,
                    "column": 2,
                    "severity": 3,
                    "message": 4,
                    "loop": true
                }
            ]
        }
    ]
}
